{
    "name": "harlekoy/laravel-apidocs",
    "description": "Replacement for Swagger. It is built for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Harlequin Doyon",
            "email": "harlequin.doyon@gmail.com",
            "homepage": "https://www.linkedin.com/in/harlequin-doyon-6a0318100/"
        }
    ],
    "homepage": "https://github.com/harlekoy/apidocs",
    "keywords": ["Laravel", "API Docs"],
    "require": {
        "illuminate/support": "~5",
        "laravel/framework": "5.7.*|5.8.*",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Harlekoy\\ApiDocs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Harlekoy\\ApiDocs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Harlekoy\\ApiDocs\\Providers\\ApiDocsServiceProvider"
            ],
            "aliases": {
                "ApiDocs": "Harlekoy\\ApiDocs\\Facades\\ApiDocs"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
